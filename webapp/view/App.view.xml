<mvc:View controllerName="be.hogent.aalst.helloworld.controller.App"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Shell id="shell">
        <App id='app'>
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>
                        <Button text="Hello world" type="Accept" press="onpress" />
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
